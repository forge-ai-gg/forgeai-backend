			}
		}
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@elizaos/core": "workspace:*",
		"@elizaos/plugin-birdeye": "workspace:*",
		"@prisma/client": "^5.22.0",
		"@solana/web3.js": "npm:@solana/web3.js@1.95.8",
		"@types/body-parser": "1.19.5",
		"@types/cors": "2.8.17",
		"@types/express": "5.0.0",
		"body-parser": "1.20.3",
		"cors": "2.8.5",
		"envalid": "^8.0.0",
		"multer": "1.4.5-lts.1",
